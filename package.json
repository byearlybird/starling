{
	"name": "@byearlybird/starling-repo",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"license": "MIT",
	"workspaces": [
		"packages/**"
	],
	"scripts": {
		"build": "bun run build:core && bun run build:plugins-poll && bun run build:plugins-query && bun run build:plugins-unstorage",
