{
	"name": "@byearlybird/flock",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js"
		},
		"./solid": {
			"types": "./dist/solid/index.d.ts",
			"import": "./dist/solid/index.js"
		},
		"./sync": {
			"types": "./dist/sync/index.d.ts",
			"import": "./dist/sync/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run build:js && bun run build:types",
		"build:js": "bun build lib/index.ts lib/react/index.ts lib/solid/index.ts lib/sync/index.ts --outdir=dist --target=node --format=esm",
		"build:types": "tsc --project tsconfig.build.json",
		"prepublishOnly": "bun run build"
	},
	"peerDependencies": {
		"typescript": "^5",
		"react": "^19",
		"react-dom": "^19",
		"solid-js": "^1.9.9",
		"unstorage": "^1.17.1"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"solid-js": {
			"optional": true
		}
	},
	"dependencies": {
		"flat": "^6.0.1",
		"mitt": "^3.0.1",
		"ulid": "^3.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/bun": "latest",
		"tinybench": "^5.0.1",
		"typescript": "^5"
	}
}
