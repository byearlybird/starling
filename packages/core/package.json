{
	"name": "@byearlybird/starling",
	"version": "0.9.3",
	"description": "Lightweight local-first data store with automatic cross-device sync. Plain JavaScript queries, framework-agnostic, zero dependencies. Offline-first apps without the complexity, in just 4KB.",
	"type": "module",
	"license": "MIT",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./plugin-unstorage": {
			"types": "./dist/plugins/unstorage/plugin.d.ts",
			"import": "./dist/plugins/unstorage/plugin.js",
			"default": "./dist/plugins/unstorage/plugin.js"
		},
		"./crdt": {
			"types": "./dist/crdt/index.d.ts",
			"import": "./dist/crdt/index.js",
			"default": "./dist/crdt/index.js"
		},
		"./store": {
			"types": "./dist/store/bundle.d.ts",
			"import": "./dist/store/bundle.js",
			"default": "./dist/store/bundle.js"
		},
		"./db": {
			"types": "./dist/db/bundle.d.ts",
			"import": "./dist/db/bundle.js",
			"default": "./dist/db/bundle.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run build.ts",
		"prepublishOnly": "bun run build.ts"
	},
	"peerDependencies": {
		"unstorage": "^1.17.1"
	},
	"peerDependenciesMeta": {
		"unstorage": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
